{
  "version": "0.0.0",
  "name": "skypin",
  "repository": "MarshallCB/skypin",
  "description": "Convert NPM imports into Skypack Pinned URL's",
  "keywords": [],
  "license": "MIT",
  "author": {
    "name": "Marshall Brandt",
    "email": "hey@m4r.sh",
    "url": "https://m4r.sh"
  },
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "test": "uvu -r esm test"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^10.0.0",
    "esm": "^3.2.25",
    "rollup": "^2.33.3",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2",
    "uvu": "^0.4.1"
  },
  "dependencies": {
    "escalade": "^3.1.1",
    "find-up": "^5.0.0",
    "node-fetch": "^2.6.1",
    "path": "^0.12.7",
    "resolve-cwd": "^3.0.0",
    "resolve-from": "^5.0.0",
    "umap": "^1.0.2"
  }
}
